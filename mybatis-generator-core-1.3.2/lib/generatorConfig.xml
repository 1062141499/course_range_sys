		<table tableName="course_range_info" domainObjectName="CourseRangeInfo" >
			<generatedKey column="id" sqlStatement="MySql" identity="true"/>
		</table>
		<table tableName="stu_range_info" domainObjectName="StuRangeInfo" >
			<generatedKey column="id" sqlStatement="MySql" identity="true"/>
		</table>

    </context>  
</generatorConfiguration>  
